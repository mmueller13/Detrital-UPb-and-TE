    "        print(\"\\033[1mDataFrame not saved.\\033[0m\")\n",
    "else:\n",
    "    print(\"\\033[1mDataFrame not saved.\\033[0m\")\n",
    "\n",
    "\n",
    "# Display the concatenated DataFrame \n",
    "pd.set_option('display.max_columns', None)\n",
